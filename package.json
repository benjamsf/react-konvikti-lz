{
  "name": "konvikti-lz",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "build:reveal": "node reveal.build.cjs",
    "build:reveal:inline": "npm run build:reveal && sh ./build.sh",
    "build:vite": "tsc && vite build",
    "dev:reveal-watch": "concurrently -k -n REVEAL -c magenta \"nodemon --watch src/decks --ext md --ignore src/decks/tmp-*.md --exec 'npm run build:reveal:inline'\"",
    "dev": "concurrently -k -n VITE,REVEAL -c cyan,magenta \"npm run dev:vite\" \"npm run dev:reveal-watch\"",
    "build": "npm run build:reveal:inline && npm run build:vite",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write ./",
    "preview": "vite preview"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@kinde-oss/kinde-auth-react": "^4.0.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/themes": "^2.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "classnames": "^2.3.2",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.16",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-chained-backend": "^4.6.2",
    "i18next-localstorage-backend": "^4.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.2",
    "react-insta-stories": "^2.6.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.14.2",
    "reveal.js": "^5.0.5",
    "swiper": "^11.1.4",
    "tailwind-variants": "^0.2.1",
    "ua-parser-js": "^1.0.39",
    "yup": "^1.3.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.17",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.1.2",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "html-minifier-terser": "^7.2.0",
    "imagemin": "^9.0.0",
    "imagemin-cli": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "inliner": "^1.13.1",
    "lint-staged": "^13.2.3",
    "marked": "^15.0.8",
    "nodemon": "^2.0.22",
